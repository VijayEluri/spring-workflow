<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <bean id="flowDefinition" class="org.springmodules.so.core.FlowDefinition">
        <constructor-arg>
            <set>
                <ref local="state1"/>
                <ref local="state2"/>
                <ref local="state3"/>
            </set>
        </constructor-arg>
    </bean>

    <bean id="state1" class="org.springmodules.so.core.StateDefinition">
        
    </bean>

    <bean id="state2" class="org.springmodules.so.core.StateDefinition">

    </bean>

    <bean id="state3" class="org.springmodules.so.core.StateDefinition">

    </bean>

</beans>
